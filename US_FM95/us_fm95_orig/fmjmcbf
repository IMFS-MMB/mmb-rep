EQUATION > pdot10
EQTYPE >   IMPOSED
EQ >       pdot10   = LAG(pdot9,1)

EQUATION > contr1
EQTYPE >   IMPOSED
EQ >       contr1   = LAG(contr,1)

EQUATION > contr2
EQTYPE >   IMPOSED
EQ >       contr2   = LAG(contr1,1)

EQUATION > index1
EQTYPE >   IMPOSED
EQ >       index1   = LAG(index,1)

EQUATION > index2
EQTYPE >   IMPOSED
EQ >       index2   = LAG(index1,1)

EQUATION > index3
EQTYPE >   IMPOSED
EQ >       index3   = LAG(index2,1)

EQUATION > pdotf4
EQTYPE >   IMPOSED
EQ >       pdotf4  = LEAD(pdot,4)

EQUATION > pdotf8
EQTYPE >   IMPOSED
EQ >       pdotf8  = LEAD(pdot,8)

EQUATION > pdotf12
EQTYPE >   IMPOSED
EQ >       pdotf12  = LEAD(pdot,12)

EQUATION > pdotf16
EQTYPE >   IMPOSED
EQ >       pdotf16  = LEAD(pdot,16)

EQUATION > pdotf20
EQTYPE >   IMPOSED
EQ >       pdotf20  = LEAD(pdot,20)

EQUATION > one
EQTYPE >   IMPOSED
EQ >       one = LAG(one,1)

EQUATION > pdot_
EQTYPE >   STOCH
EQ >       pdot_ = 0*one

EQUATION > rff_
EQTYPE >   STOCH
EQ >       rff_ = 0*one

EQUATION > ygap_
EQTYPE >   STOCH
EQ >       ygap_ = 0*one

END
